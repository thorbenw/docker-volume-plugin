{
  "description": "Volume plugin for Docker",
  "documentation": "https://github.com/thorbenw/docker-volume-plugin",
  "entrypoint": [
    "/docker-volume-plugin"
  ],
  "env": [
    {
      "name": "LOG_LEVEL",
      "settable": [
        "value"
      ],
      "value": "debug"
    },
    {
      "name": "LOG_SOURCE",
      "settable": [
        "value"
      ],
      "value": "false"
    },
    {
      "name": "VOLUME_PROCESS_BINARY",
      "settable": [
        "value"
      ],
      "value": "/testVolumeProcess"
    },
    {
      "name": "VOLUME_PROCESS_RECOVERY_MODE",
      "settable": [
        "value"
      ],
      "value": "Restart"
    },
    {
      "name": "VOLUME_PROCESS_RECOVERY_MAX_PER_MIN",
      "settable": [
        "value"
      ],
      "value": "2"
    }
  ],
  "PropagatedMount": "/data",
  "network": {
    "type": "host"
  },
  "interface": {
    "types": ["docker.volumedriver/1.0"],
    "socket": "plugin.sock"
  },
  "linux": {
    "capabilities": ["CAP_SYS_ADMIN"]
  }
}
